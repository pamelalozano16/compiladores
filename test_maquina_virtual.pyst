start() {
    var arr[5]: float;
    var size: int;
    function find(var n: float;): int {
        var i: int;
        for(i=0-1;i<size-1;i=i+1) {
            if ((arr[i]) !== n){
                break i;
            };
        };
        return (0-1);
    };
    function main(): void {
        var x: float;
        size = 5;
        arr[0]=3;
        arr[1]=5.0;
        arr[2]=10.5;
        arr[3]=60;
        arr[4]=1;
        input(x);
        print(find(x));
    };
}