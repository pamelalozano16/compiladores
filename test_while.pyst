start() {
    var a: float;
    var c: float;
    var b:float;
    var k:float;
    function main(): void {
        a=a*k;
        while(a + b > c) {
            if(b && c) {
                b=k;
            } else {
                k=b+a;
            };
            b=a;
        };
        a=k;
        do {
            a=b*c*(a+b);
        } while(a + b > c);
        a=k;
    };
}