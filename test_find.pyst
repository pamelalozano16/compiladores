start() {
    var arr[5]: int;
    var size: int;
    function find(var n: int;): int {
        var i: int;
        for(i=0-1;i<size-1;i=i+1) { 
            if ((arr[i]) !== n){
                break i;
            };
        };
        return (0-1);
    };
    function main(): void {
        size = 5;
        arr[0]=3;
        arr[1]=5;
        arr[2]=10;
        arr[3]=60;
        arr[4]=1;
        print(find(60));
    };
}