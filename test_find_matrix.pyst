        size2 = 4;
        arr[0][0]=3;
        arr[0][1]=70;
        arr[0][2]=91;
        arr[0][3]=43;
        arr[1][0]=87;
        arr[1][1]=2;
        arr[1][2]=11.2;
        arr[1][3]=49;
        arr[2][0]=333;
        arr[2][1]=10001;
        arr[2][2]=203;
